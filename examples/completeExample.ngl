ngless "0.0"
input = fastq('../samples/sample_1.fq')
input = unique(input, max_copies=2)
preprocess(input) using |read|:
    read = read[3:]
    read = substrim(read, min_quality=26)
    if len(read) < 20:
        discard
mapped = map(input,reference='sacCer3')
write(input, ofile='../samples/filtered.sam', format={sam})
write(input, ofile='../samples/filtered.bam', format={bam})